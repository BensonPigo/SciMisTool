{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug React (Edge)",
      "type": "pwa-msedge",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "breakOnLoad": true
    },
    {
      "name": "Attach to Edge",
      "type": "pwa-msedge",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}/src"
    }
  ]
}
